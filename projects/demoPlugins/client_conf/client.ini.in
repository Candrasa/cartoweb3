;@BLURB@

cartoclientBaseUrl =  "@CARTOCLIENT_BASE_URL@"
profile = @PROFILE@

mapId = demoPlugins

; Shows a drop-down list for selecting the active project
showProjectChooser = false

;## Plugins ##
; the list of plugins to load, separated by comma
loadPlugins = exportCsv, outline, views, layerReorder, mapOverlay, checkInstall, locate, exportRtf, exportDxf @ROUTING_PLUGINS@

;## Internationalization ##
defaultLang = fr
langList = fr, en

; Indicates how to render toolbar
toolbarRendering = outline

; Views management
viewOn = true
viewStorage = file     ;possible values are 'file' and 'db'
viewablePlugins = location, layers, query, outline
viewMetas = author
viewAuth = all

; toolPicker
toolPickerOn = true
