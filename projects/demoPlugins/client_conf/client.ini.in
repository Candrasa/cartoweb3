;@BLURB@

cartoclientBaseUrl =  "@CARTOCLIENT_BASE_URL@"
profile = @PROFILE@

mapId = demoPlugins

; Shows a drop-down list for selecting the active project
showProjectChooser = false

;## Plugins ##
; the list of plugins to load, separated by comma
loadPlugins = exportCsv, outline, views, layerReorder, demoLocation, checkInstall
;routing, demoRouting => to uncomment to integrate routing fonctionalities

;## Internationalization ##
defaultLang = fr
langList = fr, en

; Indicates how to render toolbar
toolbarRendering = class

; Views management
viewOn = true
viewStorage = file ;possible values are 'file' and 'db'
viewablePlugins = location, layers, query, outline
viewMetas = author
viewAuth = all
;viewSavedRequest = personalVar1, personalVar2